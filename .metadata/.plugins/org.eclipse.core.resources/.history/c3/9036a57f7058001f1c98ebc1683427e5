@ControllerAdvice
public class GlobalExcepcionHandler extends ResponseEntityExceptionHandler{

	@ExceptionHandler(ResourceNotFoundExcepcion.class)
	public ResponseEntity<Object> handlerResourceNotFoundExcepcion(){
		
	}
	
	
}
